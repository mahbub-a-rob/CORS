{
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.2.0-*",
      "type": "platform"
    },
    "Microsoft.AspNetCore.Server.Kestrel": "1.2.0-*",
    "Microsoft.Extensions.Logging.Console": "1.2.0-*",
    "Microsoft.AspNetCore.Cors": "1.2.0-*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "publishOptions": {
    "include": [
      "wwwroot"
    ]
  }
}